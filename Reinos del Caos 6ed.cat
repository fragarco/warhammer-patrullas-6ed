        <selectionEntry id="5a27-0c52-0e11-486e" name="Piedra de energía" hidden="false" collective="false" import="true" type="upgrade">
          <profiles>
            <profile id="82b6-10e4-9467-8666" name="Piedra de energía" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">Un solo uso. +2 dados para lanzar un hechizo. Este objeto puede repetirse en el mismo personaje o en otros.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name="puntos" typeId="53ea-00f4-9046-81ff" value="25.0"/>
