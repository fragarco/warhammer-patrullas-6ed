          </constraints>
          <profiles>
            <profile id="e887-ad2f-b8d8-228f" name="Familiar de energía" hidden="false" typeId="0912-c473-dc44-71c2" typeName="Objetos mágicos">
              <characteristics>
                <characteristic name="Propiedades mágicas" typeId="e33b-a1fc-9f43-c88d">+1 dado de energía, +1 dado de dispersión.</characteristic>
